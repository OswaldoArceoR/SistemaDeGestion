<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="300.0" prefWidth="400.0" spacing="10.0" style="-fx-background-color: #3050bd;" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.resources.org.example.sistemaproyec.Vista.MenuPrincipalVista">
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>

    <ImageView fitHeight="80.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
        <image>
            <Image url="/main/resources/org/example/sistemaproyec/un_lobo_ingeniero.png"/>
        </image>
    </ImageView>

    <Button text="Gestionar Productos" onAction="#irAVistaProducto">
        <font>
            <Font name="Century Gothic" size="16.0"/>
        </font>
    </Button>

</VBox>
