<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="300.0" prefWidth="400.0" spacing="10.0" style="-fx-background-color: #3050bd;" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.resources.org.example.sistemaproyec.Vista.MenuPrincipalVista">
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
    <ImageView fitHeight="200.0" fitWidth="200.0">
        <image>
            <Image url="@../un_lobo_ingeniero-removebg-preview.png" />
        </image>
    </ImageView>

    <Button onAction="#abrirClienteVista" prefHeight="37.0" prefWidth="159.0" text="Gestionar Clientes">
        <font>
            <Font name="Segoe UI Semibold" size="16.0" />
        </font>
    </Button>
    <Button onAction="#irAHistorialVentas" text="Historial de Ventas">
        <font>
            <Font name="Segoe UI Semibold" size="16.0" />
        </font>
    </Button>
    <Button onAction="#irAVistaProducto" prefHeight="37.0" prefWidth="156.0" text="Empleados">
        <font>
            <Font name="Segoe UI Semibold" size="16.0" />
        </font>
    </Button>
    <Button onAction="#realizarPedido" prefHeight="38.0" prefWidth="156.0" text="Pedidos">
        <font>
            <Font name="Segoe UI Semibold" size="16.0" />
        </font>
    </Button>
    <Button onAction="#abrirReporteVentaYFinanzas" prefHeight="38.0" prefWidth="220.0" text="Reporte de venta y finanzas">
        <font>
            <Font name="Segoe UI Semibold" size="16.0" />
        </font>
    </Button>
</VBox>
